{
    "name": "Annotator",
    "version": "0.0.1",
    "description": "",
    "permissions": ["webNavigation", "declarativeContent", "activeTab", "storage",
        "tabs", "<all_urls>"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["drawing.js", "annotOps.js", "content.js"]
        }
    ],
    "browser_action": {
    },
    "web_accessible_resources": [
        "images/*.png",
        "displayPdf.html"
    ],
    "manifest_version": 2
}
